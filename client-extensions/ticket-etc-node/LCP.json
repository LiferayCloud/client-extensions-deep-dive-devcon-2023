{
	"cpu": 1,
	"env": {
		"LIFERAY_ROUTES_CLIENT_EXTENSION": "/etc/liferay/lxc/ext-init-metadata",
		"LIFERAY_ROUTES_DXP": "/etc/liferay/lxc/dxp-metadata"
	},
	"environments": {
		"infra": {
			"deploy": false
		}
	},
	"id": "ticketetcnode",
	"kind": "Deployment",
	"livenessProbe": {
		"httpGet": {
			"path": "/ready",
			"port": 3001
		}
	},
	"loadBalancer": {
		"targetPort": 3001
	},
	"memory": 512,
	"readinessProbe": {
		"httpGet": {
			"path": "/ready",
			"port": 3001
		}
	},
	"scale": 1
}