{
	"concurrencyPolicy": "Forbid",
	"cpu": 1,
	"env": {
		"LIFERAY_ROUTES_CLIENT_EXTENSION": "/etc/liferay/lxc/ext-init-metadata",
		"LIFERAY_ROUTES_DXP": "/etc/liferay/lxc/dxp-metadata"
	},
	"environments": {
		"infra": {
			"deploy": false
		}
	},
	"id": "__PROJECT_ID__",
	"kind": "CronJob",
	"memory": 1024,
	"scale": 1,
	"schedule": "*/15 * * * *"
}